{
    "compilerOptions": {
        "target": "ES6",
        "lib": ["ES2021"],
        "jsx": "react",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "allowSyntheticDefaultImports": true,
        "baseUrl": "./",
        "paths": {
            "*": ["*"]
        },
        "noImplicitAny": false,
        "strict": true
    }
}
